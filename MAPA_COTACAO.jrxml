	<queryString>
		<![CDATA[SELECT TGFCOT.NUMCOTACAO
     , CONVERT(VARCHAR,TGFCOT.CODEMP)+' - '+TSIEMP.RAZAOABREV AS EMPRESA
	 , CONVERT(VARCHAR,TGFCOT.CODCENCUS)+' - '+TSICUS.DESCRCENCUS AS AREA
	 , TGFCOT.DHINIC AS DT_COTACAO
	 , CONVERT(VARCHAR,TGFCOT.CODUSURESP)+' - '+TSIUSU.NOMEUSU AS COMPRADOR
	 , TGFCOT.OBSERVACAO
     , SANKHYA.FN_TOP_FOR_COTACAO(TGFCOT.NUMCOTACAO, 1) AS CODPARC_1
	 , FORN_1.NOMEPARC AS NOMEPARC_1
	 , SANKHYA.FN_TOP_FOR_COTACAO(TGFCOT.NUMCOTACAO, 2) AS CODPARC_2
	 , FORN_2.NOMEPARC AS NOMEPARC_2
	 , SANKHYA.FN_TOP_FOR_COTACAO(TGFCOT.NUMCOTACAO, 3) AS CODPARC_3
	 , FORN_3.NOMEPARC AS NOMEPARC_3
	 , TGFITC.CODPROD
	 , RTRIM(TGFPRO.DESCRPROD) + ' ' + TGFITC.CONTROLE AS DESCRPROD
	 , TGFITC.CODVOL
	 , TGFITC.QTDCOTADA
	 , ISNULL(FORN_1.PRECO,0) AS PRECO_1
	 , ISNULL(FORN_1.PRECO,0)-ISNULL(FORN_1.VLRDESC,0) AS PRECO_NEG_1
	 , ISNULL(FORN_1.QTDPARC,0) AS QTDPARC_1
	 , ISNULL(FORN_1.PRAZOPARC,0) AS PRAZOPARC_1
	 , ISNULL(FORN_1.PRAZOENTREGA,0) AS PRAZOENTREGA_1
	 , ISNULL(FORN_1.VLRDESC,0) AS VLRDESC_1
	 , SANKHYA. F_DESCROPC('TGFITC','AD_FORMAPAGTO',FORN_1.AD_FORMAPAGTO) AS FORMAPAGTO_1
	 , CASE WHEN FORN_1.SITUACAO = 'A' THEN 1 ELSE 0 END AS SITUACAO_1
	 , ISNULL(FORN_2.PRECO,0) AS PRECO_2
	 , ISNULL(FORN_2.PRECO,0)-ISNULL(FORN_2.VLRDESC,0) AS PRECO_NEG_2
	 , ISNULL(FORN_2.QTDPARC,0) AS QTDPARC_2
	 , ISNULL(FORN_2.PRAZOPARC,0) AS PRAZOPARC_2
	 , ISNULL(FORN_2.PRAZOENTREGA,0) AS PRAZOENTREGA_2
	 , ISNULL(FORN_2.VLRDESC,0) AS VLRDESC_2
	 , SANKHYA. F_DESCROPC('TGFITC','AD_FORMAPAGTO',FORN_2.AD_FORMAPAGTO) AS FORMAPAGTO_2
	 , CASE WHEN FORN_2.SITUACAO = 'A' THEN 1 ELSE 0 END AS SITUACAO_2
	 , ISNULL(FORN_3.PRECO,0) AS PRECO_3
     , ISNULL(FORN_3.PRECO,0)-ISNULL(FORN_3.VLRDESC,0) AS PRECO_NEG_3
	 , ISNULL(FORN_3.QTDPARC,0) AS QTDPARC_3
	 , ISNULL(FORN_3.PRAZOPARC,0) AS PRAZOPARC_3
	 , ISNULL(FORN_3.PRAZOENTREGA,0) AS PRAZOENTREGA_3
	 , ISNULL(FORN_3.VLRDESC,0) AS VLRDESC_3
     , SANKHYA. F_DESCROPC('TGFITC','AD_FORMAPAGTO',FORN_3.AD_FORMAPAGTO) AS FORMAPAGTO_3
	 , CASE WHEN FORN_3.SITUACAO = 'A' THEN 1 ELSE 0 END AS SITUACAO_3
     , LEAST(ISNULL(FORN_1.PRECO,0), ISNULL(FORN_2.PRECO,0), ISNULL(FORN_3.PRECO,0)) AS MELHOR_PRECO
	 , LEAST(ISNULL(FORN_1.PRECO,0)-ISNULL(FORN_1.VLRDESC,0), ISNULL(FORN_2.PRECO,0)-ISNULL(FORN_2.VLRDESC,0), ISNULL(FORN_3.PRECO,0)-ISNULL(FORN_3.VLRDESC,0)) AS MELHOR_PRECO_NEG
  FROM SANKHYA.TGFCOT (NOLOCK)
  INNER JOIN SANKHYA.TSIEMP (NOLOCK) ON TGFCOT.CODEMP = TSIEMP.CODEMP
  INNER JOIN SANKHYA.TSICUS (NOLOCK) ON TGFCOT.CODCENCUS = TSICUS.CODCENCUS
  INNER JOIN SANKHYA.TSIUSU (NOLOCK) ON TGFCOT.CODUSURESP = TSIUSU.CODUSU
  INNER JOIN SANKHYA.TGFITC (NOLOCK) ON TGFCOT.NUMCOTACAO = TGFITC.NUMCOTACAO AND CABECALHO = 'S'
  INNER JOIN SANKHYA.TGFPRO (NOLOCK) ON TGFITC.CODPROD = TGFPRO.CODPROD
   LEFT JOIN (SELECT TGFITC.PRECO
                   , TGFITC.CODPARC
				   , TGFITC.NUMCOTACAO
				   , TGFITC.CODPROD
				   , TGFPAR.NOMEPARC
				   , CASE WHEN ISNULL(TGFITC.QTDPARCPAGT,0) = 0 THEN 1 ELSE TGFITC.QTDPARCPAGT END  AS QTDPARC
				   , TGFITC.PRAZOPARC
				   , TGFITC.PRAZOENTREGA
				   , TGFITC.VLRDESC
				   , TGFITC.AD_FORMAPAGTO
				   , TGFITC.SITUACAO
                FROM SANKHYA.TGFITC (NOLOCK)
				INNER JOIN SANKHYA.TGFPAR (NOLOCK) ON TGFITC.CODPARC = TGFPAR.CODPARC
				WHERE TGFITC.CABECALHO = 'N') FORN_1 ON FORN_1.NUMCOTACAO = TGFCOT.NUMCOTACAO AND FORN_1.CODPARC = SANKHYA.FN_TOP_FOR_COTACAO(TGFCOT.NUMCOTACAO, 1) AND FORN_1.CODPROD = TGFITC.CODPROD
   LEFT JOIN (SELECT TGFITC.PRECO
                   , TGFITC.CODPARC
				   , TGFITC.NUMCOTACAO
				   , TGFITC.CODPROD
				   , TGFPAR.NOMEPARC
				   , CASE WHEN ISNULL(TGFITC.QTDPARCPAGT,0) = 0 THEN 1 ELSE TGFITC.QTDPARCPAGT END  AS QTDPARC
				   , TGFITC.PRAZOPARC
				   , TGFITC.PRAZOENTREGA
				   , TGFITC.VLRDESC
				   , TGFITC.AD_FORMAPAGTO
				   , TGFITC.SITUACAO
                FROM SANKHYA.TGFITC (NOLOCK)
				INNER JOIN SANKHYA.TGFPAR (NOLOCK) ON TGFITC.CODPARC = TGFPAR.CODPARC
				WHERE TGFITC.CABECALHO = 'N') FORN_2 ON FORN_2.NUMCOTACAO = TGFCOT.NUMCOTACAO AND FORN_2.CODPARC = SANKHYA.FN_TOP_FOR_COTACAO(TGFCOT.NUMCOTACAO, 2) AND FORN_2.CODPROD = TGFITC.CODPROD
   LEFT JOIN (SELECT TGFITC.PRECO
                   , TGFITC.CODPARC
				   , TGFITC.NUMCOTACAO
				   , TGFITC.CODPROD
				   , TGFPAR.NOMEPARC
				   , CASE WHEN ISNULL(TGFITC.QTDPARCPAGT,0) = 0 THEN 1 ELSE TGFITC.QTDPARCPAGT END  AS QTDPARC
				   , TGFITC.PRAZOPARC
				   , TGFITC.PRAZOENTREGA
				   , TGFITC.VLRDESC
				   , TGFITC.AD_FORMAPAGTO
				   , TGFITC.SITUACAO
                FROM SANKHYA.TGFITC (NOLOCK)
				INNER JOIN SANKHYA.TGFPAR (NOLOCK) ON TGFITC.CODPARC = TGFPAR.CODPARC
				WHERE TGFITC.CABECALHO = 'N') FORN_3 ON FORN_3.NUMCOTACAO = TGFCOT.NUMCOTACAO AND FORN_3.CODPARC = SANKHYA.FN_TOP_FOR_COTACAO(TGFCOT.NUMCOTACAO, 3) AND FORN_3.CODPROD = TGFITC.CODPROD
 WHERE TGFCOT.NUMCOTACAO = 1757]]>
	</queryString>
